{
  "watch": ["src"],
  "verbose": true,
  "debug": true,
  "exec": "NODE_ENV=development node --inspect=5050 -r ts-node/register src/index.ts",
  "ignore": ["node_modules", "**/*.d.ts", "*.test.ts", "*.spec.ts", "fixtures/*", "test/**/*", "docs/*", "src/public"],
  "ext": "ts,gql,js",
  "inspect": true
}
