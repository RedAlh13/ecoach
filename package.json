  "license": "MIT",
  "scripts": {
    "db:new": "knex -x ts migrate:make",
    "db:up": "knex migrate:latest",
    "db:down": "knex migrate:rollback",
    "db:teardown": "knex migrate:rollback --all",
    "lint": "eslint migrations/**/*.ts --fix",
    "dev": "tsc src/index.ts --watch & nodemon dist",
    "start": "nodemon"
  },
  "dependencies": {
    "express": "^4.17.1",
    "graphile-build-pg": "^4.5.3",
    "knex": "^0.20.13",
    "pg": "^8.0.0",
    "postgraphile": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
